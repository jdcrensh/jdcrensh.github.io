    "coveralls": "^3.0.4",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.4.1",
    "jest-dom": "^3.5.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
